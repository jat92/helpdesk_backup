version: 1
disable_existing_loggers: no
formatters:
    default:
        format: "[%(process)d] %(filename)s(%(funcName)s:%(lineno)s) - %(levelname)s: %(message)s"
    console:
        format: "%(asctime)s [%(process)d] %(filename)s(%(funcName)s:%(lineno)s) - %(levelname)s: %(message)s"
    http:
        format: "%(asctime)s %(message)s"
handlers:
    syslog:
        class: logging.handlers.SysLogHandler
        level: DEBUG
        formatter: default
        address: /dev/log
        facility: local0
    stdout:
        class: logging.StreamHandler
        level: DEBUG
        formatter: console
    http-console:
        class: logging.StreamHandler
        level: INFO
        formatter: http
root:
    level: DEBUG
    handlers: [stdout]
    propogate: no
loggers:
    main:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    __main__:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    backup-fs:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    http_func:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    fs_restore:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    manage-fs:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
    db_func:
        level: DEBUG
        handlers: [stdout]
        propogate: yes
